{
    "name": "kristofvc/contact",
    "type": "component",
    "description": "This is a component that defines a contact page",
    "keywords": ["contact", "form"],
    "homepage": "http://github.com/kristofvc/Contact",
    "license": "MIT",
    "authors": [
        {
            "name": "Kristof Van Cauwenbergh",
            "email": "kristof.vancauwenbergh@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "symfony/http-foundation": ">=2.3",
        "symfony/form": ">=2.3",
        "symfony/event-dispatcher": ">=2.3"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "2.*"
    },
    "suggest": {
        "guzzlehttp/guzzle": "If you want to integrate with slack you could use guzzle to send your data to slack"
    },
    "autoload": {
        "psr-4": { "Kristofvc\\Contact\\": "" }
    }
}
